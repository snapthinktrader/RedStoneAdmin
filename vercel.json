{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "alias": ["redstoneadmin.vercel.app"],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/redstone-logo.png",
      "dest": "/redstone-logo.png"
    },
    {
      "src": "/logo.png",
      "dest": "/logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
